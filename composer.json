{
    "name": "soulaimaneyh/storage-link-route",
    "description": "storage-link-route",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Soulaimaneyh\\StorageLinkRoute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soulaimaneyh\\StorageLinkRoute\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Soulaimane yahya",
            "email": "soulaimaneyh07@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit tests/Feature",
        "php-psr": [
            "./vendor/bin/phpcs src/Providers/StorageLinkRouteServiceProvider.php --standard=PSR2"
        ]
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "dev-develop",
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^9.6"
    },
    "suggest": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soulaimaneyh\\StorageLinkRoute\\Providers\\StorageLinkRouteServiceProvider"
            ]
        }
    }
}
